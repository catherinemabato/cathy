fi

test -z "$CC" && { { echo "$as_me:1401: error: no acceptable cc found in \$PATH" >&5
echo "$as_me: error: no acceptable cc found in \$PATH" >&2;}
   { (exit 1); exit 1; }; }

# Provide some information about the compiler.
echo "$as_me:1406:" \
     "checking for C compiler version" >&5
