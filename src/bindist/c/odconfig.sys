rem config.sys for DOSEMU + DR-OpenDOS (not "DR or OpenDOS", but DR-OpenDOS)
rem must be copied to C:\
DEVICE=C:\OPENDOS\SETVER.EXE
SHELL=C:\COMMAND.COM C:\ /E:512 /P:odautoem.bat
BREAK=OFF
BUFFERS=15
FILES=20
FCBS=4,4
LASTDRIVE=Z
HISTORY=ON,512,ON
COUNTRY=1,,C:\OPENDOS\COUNTRY.SYS
device=d:\dosemu\emufs.sys
device=d:\dosemu\umb.sys
rem DOS=UMB works only after umb.sys
DOS=HIGH,UMB
devicehigh=d:\dosemu\ems.sys
devicehigh=d:\dosemu\cdrom.sys
